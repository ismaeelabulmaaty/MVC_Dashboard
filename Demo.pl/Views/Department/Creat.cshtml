@model Department
@{
    ViewData["Title"] = "Creat";
}

<center class="mt-5 pt-3">
    <h1>Creat New Department</h1>
    <br />
    <br />
</center>
<div class="w-75 m-auto rounded shadow bg-light mb-5">
    <form asp-action="Creat" class="p-5 mb-3">

        <div class="form-group">

            <label class="col-form-label" asp-for="Code"></label>
            <input type="text" asp-for="Code" class="form-control" />
            <span asp-validation-for="Code" class="text-danger"></span>

        </div>

        <div class="form-group">
            <label class="col-form-label" asp-for="Name"></label>
            <input type="text" asp-for="Name" class="form-control" />
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>

        <div class="form-group">
            <label class="col-form-label" asp-for="DateOfCreation"></label>
            <input   asp-for="DateOfCreation" class="form-control" />
        </div>

        <input type="submit" value="Creat" class="btn btn-outline-primary" />
        <a asp-action="Index" class="btn btn-outline-secondary text-dark">Back To List</a>

    </form>
</div>
@section validationScript {
   @*  <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
 *@
    <partial name="_ValidationScriptsPartial" />
}

